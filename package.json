{
  "name": "fdk-admin-gui",
  "version": "0.1.0",
  "description": "An administration application for Fellesdatakatalog",
  "private": true,
  "engines": {
    "node": ">= 10.16.0"
  },
  "scripts": {
    "start": "npm-run-all start:webpack",
    "start:webpack": "webpack-dev-server --hot --inline --config-register @babel/register --config webpack/dev.config.js",
    "build:dev": "npm-run-all build:webpack:dev",
    "build:prod": "npm-run-all build:webpack:prod",
    "build:webpack:dev": "webpack --config-register @babel/register --config webpack/dev.config.js",
    "build:webpack:prod": "webpack --config-register @babel/register --config webpack/prod.config.js",
    "build:webpack:stats": "webpack --config-register @babel/register --config webpack/stats.config.js",
    "type:check": "tsc",
    "lint:project": "eslint \"**\"",
    "lint:project:fix": "eslint \"**\" --fix",
    "lint:staged:fix": "eslint --ext tsx,ts,jsx,js,json,md,html --fix",
    "test": "npm-run-all clean:coverage test:jest",
    "test:coverage": "npm-run-all clean:coverage test:jest:coverage",
    "test:jest": "jest --config jest.config.js",
    "test:jest:coverage": "jest --config jest.config.js --coverage",
    "clean:coverage": "rm -rf coverage",
    "test:watch": "jest --config jest.config.js --watchAll",
    "bundle:analyse": "npm-run-all build:webpack:stats",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Informasjonsforvaltning/fdk-admin-gui.git"
  },
  "author": {
    "name": "Alexander Zakharov (azaxarov)",
    "email": "alexander.zakharov@capgemini.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Informasjonsforvaltning/fdk-admin-gui/issues"
  },
  "homepage": "https://github.com/Informasjonsforvaltning/fdk-admin-gui#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "formik": "^2.1.5",
    "immutable": "^4.0.0-rc.12",
    "keycloak-js": "^10.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.7",
    "styled-components": "^5.1.1",
    "validator": "^13.1.1",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.1",
    "@hot-loader/react-dom": "^16.13.0",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@types/jest": "^26.0.10",
    "@types/jwt-decode": "^2.2.1",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.2",
    "@types/validator": "^13.1.0",
    "@types/webpack-env": "^1.15.2",
    "@types/yup": "^0.29.5",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.1.2",
    "copy-webpack-plugin": "^6.0.3",
    "cz-conventional-changelog": "^3.2.0",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-html": "^6.0.3",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-svg-loader": "^3.0.3",
    "source-map-loader": "^1.0.1",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.1"
  }
}
